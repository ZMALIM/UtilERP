{
    "name": "utiles/softerp",
    "description": "Crea menus y sub menus",
    "authors": [
        {
            "name": "Leonel HS",
            "email": "lionelsh.salazar@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1.3",
        "laravelcollective/html": "5.7.*",
        "illuminate/translation": "5.7.*"
    },
    "require-dev": {
        "phpspec/phpspec": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "UtilERP\\": "src/"
        },
        "files": [
          "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "UtilERP\\HtmlServiceProvider"
            ],
            "aliases": {
                "Menu": "UtilERP\\Facades\\Menu"
            }
        }
    }
}
